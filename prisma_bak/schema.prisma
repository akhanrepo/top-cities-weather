generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Weather {
  id          Int      @id @default(autoincrement())
  city        String   @unique
  temperature Int
  condition   String
  humidity    Int
  windSpeed   Int
  lastUpdated DateTime @default(now())
}
